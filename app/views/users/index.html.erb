<% @users.each do |user| %>
	<div class="demo-card-wide mdl-card mdl-shadow--2dp">
		<div class="mdl-card__supporting-text">
			<div class="container">
				<div class="row"><!-- PARENT INFO ROW -->
					<div class="col-md-2">
						<%= image_tag "#{rand(7)}.png", :class => "avatar-parent" %>
					</div>
					<div class="col-md-7" id="parent-header">
						<h4 class="parent_name"><%= user[:username] %></h4>
						<h6><%= user[:city]  %></h6>
						<p><%= user[:bio]  %></p>
					</div>
				</div> <!-- end row -->
				<div class="row">
					
					<!--TRY TO FIND PARENT ASSOCIATED WITH CHILD THROUGH CHILD-->


				  <div class="col-sm-5">
				    <div class="row">
				      <div class="col-sm-2">
				        <!-- Image goes here -->
				        <%= image_tag "#{rand(7)}.png", :class => "avatar-child" %>
				      </div>
				      <div class="col-sm-10">
				        <!-- Age, etc. goes here -->
				      </div>
				    </div>
				  </div>



				</div> <!-- end child row -->
			</div> <!-- end container -->

			<!--Begin accordion-->

			<div class="accordion">
			    <div class="panel-group" id="accordion">
			        <div>
			            <div id="<%=user.id%>" class="panel-collapse collapse">
			                <div class="panel-body">
			                	<h6 class="text-center">Bio</h6>
			                    <p><%=user.bio%></p>
			                </div>
			            </div>
			        </div>

			    </div>
			    <button class="mdl-button mdl-js-button mdl-button--fab mdl-js-ripple-effect mdl-button--colored pull-right" data-toggle="collapse" data-parent="#accordion" href="#<%=user.id%>">
			      <i class="material-icons">keyboard_arrow_down</i>
			    </button>
			</div>

			<!--end accordion-->
		</div> <!-- close mdl-card__supporting-text -->
	  <div class="mdl-card__actions mdl-card--border">
	    <a class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect">
	      View More
	    </a>
	  </div>
	</div> <!-- CLOSE CARD -->
<% end %>